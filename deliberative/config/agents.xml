<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->

	<!-- #### DELIBERATIVE AGENTS ### -->
	<agent name="deliberative-astar">
		<set class-path="bin/"/>
		<set class-name="deliberativeAgent.DeliberativeAgent"/>
		<set algorithm="ASTAR"/>
	</agent>
	<agent name="deliberative-bfs">
		<set class-path="bin/"/>
		<set class-name="deliberativeAgent.DeliberativeAgent"/>
		<set algorithm="BFS"/>
	</agent>

	<agent name="deliberative-naive">
		<set class-path="bin/"/>
		<set class-name="template.DeliberativeTemplate"/>
		<set algorithm="BFS"/>
	</agent>


</agents>
